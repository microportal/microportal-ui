{
  "name": "@microportal-ui/core",
  "imageName": "microportal/core-ui",
  "description": "Core ui microfrontend for microportal.",
  "version": "1.0.0",
  "author": "Rafael Ramos",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --port 9002 --hot --host 0.0.0.0",
    "build": "NODE_ENV=production webpack -p --progress && docker build -t $npm_package_imageName:$npm_package_version . --no-cache --pull"
  },
  "keywords": [
    "react",
    "redux",
    "microfrontend",
    "single-spa",
    "single-spa-react"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.5.5",
    "babel-loader": "^8.0.6",
    "file-loader": "^4.2.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.3",
    "@material-ui/icons": "^4.2.1",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "single-spa": "^4.3.7",
    "single-spa-react": "^2.10.2"
  }
}
